{
  "name": "react-next-boilerplate",
  "version": "0.1.0",
  "description": "Un peu de code pour commencer rapidement mes projets React + NextJs",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/spidergon/react-next-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spidergon/react-next-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/spidergon/react-next-boilerplate/issues"
  },
  "keywords": [
    "next.js",
    "react",
    "apollo",
    "boilerplate",
    "styled-components"
  ],
  "scripts": {
    "dev": "npm run clean-babel && next -p 7777",
    "build": "npm run clean-babel && next build",
    "start": "npm run clean-babel && next start",
    "test": "NODE_ENV=test jest --watch",
    "sandard": "standard --parser babel-eslint",
    "clean-babel": "rm -rf ./node_modules/.cache/babel-loader"
  },
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.20",
    "apollo-client": "^2.4.5",
    "babel-core": "^7.0.0-beta.40",
    "babel-plugin-import-graphql": "2.6.2",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "next": "^7.0.2",
    "next-with-apollo": "^3.2.2",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.3",
    "react-dropzone": "^7.0.1",
    "react-jss": "^8.6.1",
    "react-simplemde-editor": "^3.6.22",
    "slugs": "^0.1.3",
    "styled-components": "4.1.1",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "enzyme-to-json": "^3.3.4",
    "graphql-tools": "^4.0.3",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ],
          "next/babel"
        ]
      }
    },
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      "import-graphql"
    ]
  }
}
